{
  "name": "qikker-online/nova-menu-builder",
  "description": "Menu Builder Tool for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "menu",
    "builder",
    "optimistdigital",
    "qikker"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Qikker Online",
      "email": "marijn@qikkeronline.nl",
      "role": "Maintainer"
    },
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "spatie/laravel-translatable": "^4.2",
    "eminiarts/nova-tabs": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "QikkerOnline\\NovaMenuBuilder\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "QikkerOnline\\NovaMenuBuilder\\NovaMenuBuilderServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
